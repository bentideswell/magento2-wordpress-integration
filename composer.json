{
    "name": "fishpig/magento2-wordpress-integration",
    "description": "Securely integrate WordPress and Magento 2.",
    "version": "3.12.1",
    "require": {
        "php": ">=7.0",
        "magento/framework": "*",
        "magento/module-config": "*",
        "magento/module-store": "*",
        "magento/module-sitemap": "^100.3"
    },
    "suggest": {
        "fishpig/magento2-wordpress-integration-yoastseo": "^3.0"
    },
    "type": "magento2-module",
    "autoload": {
    	"files": [
            "registration.php",
            "cli_commands.php"
    	],
    	"psr-4": {
            "FishPig\\WordPress\\": ""
    	},
        "exclude-from-classmap": [
            "**/legacy-src/**",
            "**/wptheme/**"
        ]
    }
}
